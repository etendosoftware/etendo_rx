<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openbravo.base.model.Table" lazy="false" table="ad_table">
        <cache usage="read-write"/>

        <id name="id" type="string" column="ad_table_id">
            <generator class="org.etendorx.base.session.DalUUIDGenerator"/>
        </id>

        <property name="active" type="org.etendorx.base.session.OBYesNoType" not-null="true"
                  column="isactive"/>
        <property name="name" not-null="true"/>

        <property name="className"/>

        <property name="dataOrigin" type="string" not-null="true" column="dataorigintype"/>
        <property name="tableName"/>
        <property name="view" type="org.etendorx.base.session.OBYesNoType" column="isview"/>
        <property name="deletable" type="org.etendorx.base.session.OBYesNoType"
                  column="isdeleteable"/>
        <property name="accessLevel" column="accesslevel"/>

        <many-to-one name="thePackage" not-null="true" class="org.openbravo.base.model.Package"
                     column="ad_package_id"/>

        <property name="updated"/>

        <property name="treeType" column="treetype"/>

    </class>
</hibernate-mapping>